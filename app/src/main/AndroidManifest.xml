<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.accountmanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccountManager">
        <activity
            android:name=".activity.Modify"
            android:exported="false"
            android:label="Change Info" />
        <activity
            android:name=".activity.MainPage"
            android:exported="false"
            android:label="Personal Financier" />
        <activity
            android:name=".activity.Login"
            android:exported="false"
            android:label="Login" />
        <activity
            android:name=".activity.Setting"
            android:exported="false"
            android:label="Password" />
        <activity
            android:name=".activity.Help"
            android:exported="false"
            android:label="Help" />
        <activity
            android:name=".activity.MyNote"
            android:exported="false"
            android:label="My Note" />
        <activity
            android:name=".activity.AddNote"
            android:exported="false"
            android:label="Add Note" />
        <activity
            android:name=".activity.MySpend"
            android:exported="false"
            android:label="My Expenditure" />
        <activity
            android:name=".activity.MyIncome"
            android:exported="false"
            android:label="My Income" />
        <activity
            android:name=".activity.AddSpend"
            android:exported="false"
            android:label="New Expenditure" />
        <activity
            android:name=".activity.AddIncome"
            android:exported="false"
            android:label="New Income" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Account Manager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>